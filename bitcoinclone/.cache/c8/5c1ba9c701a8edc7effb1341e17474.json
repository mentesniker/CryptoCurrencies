nerated":{"line":127,"column":17},"source":"components/ConductTransaction.js","original":{"line":53,"column":40}},{"name":"conductTransaction","generated":{"line":127,"column":22},"source":"components/ConductTransaction.js","original":{"line":53,"column":45}},{"generated":{"line":128,"column":0},"source":"components/ConductTransaction.js","original":{"line":53,"column":5}},{"generated":{"line":128,"column":18},"source":"components/ConductTransaction.js","original":{"line":33,"column":4}},{"generated":{"line":128,"column":19},"source":"components/ConductTransaction.js","original":{"line":31,"column":3}},{"generated":{"line":128,"column":20},"source":"components/ConductTransaction.js","original":{"line":30,"column":2}},{"generated":{"line":129,"column":0},"source":"components/ConductTransaction.js","original":{"line":59,"column":2}},{"name":"Component","generated":{"line":133,"column":2},"source":"components/ConductTransaction.js","original":{"line":5,"column":33}},{"name":"ConductTransaction","generated":{"line":135,"column":15},"source":"components/ConductTransaction.js","original":{"line":62,"column":15}}],"sources":{"components/ConductTransaction.js":"import React, {Component} from 'react';\nimport {FormGroup, FormControl, Button} from 'react-bootstrap';\nimport {Link} from 'react-router-dom';\n\nclass ConductTransaction extends Component{\n\tstate = {recipient: '', amount: 0};\n\n\tupdateRecipient = event => {\n\t\tthis.setState({recipient: event.target.value});\n\t}\n\n\tupdateAmount = event => {\n\t\tthis.setState({amount: Number(event.target.value)});\t\n\t}\n\n\tconductTransaction = () => {\n\t    const { recipient, amount } = this.state;\n\n\t    fetch(`${document.location.origin}/api/transact`, {\n\t      method: 'POST',\n\t      headers: { 'Content-Type': 'application/json' },\n\t      body: JSON.stringify({ recipient, amount })\n\t    }).then(response => response.json())\n\t      .then(json => {\n\t        alert(json.message || json.type);\n\t      });\n  \t}\n\n\trender(){\n\t\treturn(\n\t\t\t<div className='ConductTransaction' style={{paddingTop: \"5%\"}}>\n\t\t\t\t<h2 style={{textAlign:\"center\"}}>Conduct a Transaction</h2>\n\t\t\t\t<div style={{width: \"50%\",margin: \"0 auto\", paddingTop:\"5%\"}}>\n\t\t\t\t\t<FormGroup>\n\t\t\t\t\t\t<label for=\"account\">Account</label>\n\t\t\t\t\t\t<FormControl\n\t\t\t\t\t\t\tid=\"account\"\n\t\t\t\t\t\t\tinput='text'\n\t\t\t\t\t\t\tplaceholder='account number'\n\t\t\t\t\t\t\tvalue={this.state.recipient}\n\t\t\t\t\t\t\tonChange={this.updateRecipient}\n\t\t\t\t\t\t/>\n\t\t\t\t\t\t<br/>\n\t\t\t\t\t\t<label for=\"quantity\" style={{paddingTop: \"5%\"}}>Quantity</label>\n\t\t\t\t\t\t<FormControl\n\t\t\t\t\t\t\tid=\"quantity\"\n\t\t\t\t\t\t\tinput='number'\n\t\t\t\t\t\t\tplaceholder='amount'\n\t\t\t\t\t\t\tvalue={this.state.amount}\n\t\t\t\t\t\t\tonChange={this.updateAmount}\n\t\t\t\t\t\t/>\n\t\t\t\t\t</FormGroup>\n\t\t\t\t\t<Button bsStyle= \"danger\" onClick={this.conductTransaction}>\n\t\t\t\t\t\tSubmit\n\t\t\t\t\t</Button>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t)\n\t}\n}\n\nexport default ConductTransaction;"},"lineCount":null}},"error":null,"hash":"773f0975c8a0a5aa21837f4e6f6c3c8d","cacheData":{"env":{}}}